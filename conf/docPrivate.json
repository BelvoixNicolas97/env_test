{
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "./"
    ],
    "exclude": [
      "./node_modules",
      "./doc"
    ]
  },
  "sourceType": "module",
  "templates": {
    "default": {
      "staticFiles": {
        "include": ["./markdown/src"]
      }
    }
  },
  "opts": {
    "recurse": true,
    "private": true,
    "package": "./package.json",
    "verbose": true,
    "tutorials": "./markdown",
    "readme": "./README.md",
    "destination": "./doc/Private"
  }
}