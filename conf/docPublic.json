{
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "./"
    ],
    "exclude": [
      "./node_modules",
      "./doc",
      "./script"
    ]
  },
  "opts": {
    "recurse": true,
    "private": false,
    "package": "./package.json",
    "verbose": true,
    "tutorials": "./markdown",
    "readme": "./README.md",
    "destination": "./doc/Public"
  }
}